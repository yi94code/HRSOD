  bottom: "conv5_3_score"
  top: "conv5_3_score"
  relu_param {
    negative_slope: 0.01
  }
}
layer {
  name: "upscore1"
  type: "Deconvolution"
  bottom: "conv5_3_score"
  top: "upscore1"
  param {
    lr_mult: 10
    decay_mult: 10
  }
  convolution_param {
    num_output: 2
    bias_term: false
    kernel_size: 8
    pad: 3
    stride: 2
    weight_filler {
      type: "bilinear"
    }
  }
}

layer {
  name: "bn_upscore1"
  type: "BatchNorm"
  bottom: "upscore1"
  top: "upscore1"
  param {
    lr_mult: 0
  }
  param {
    lr_mult: 0
  }
  param {
    lr_mult: 0
  }
}
layer {
  name: "scale_upscore1"
  type: "Scale"
  bottom: "upscore1"
  top: "upscore1"
  param {
    lr_mult: 10
