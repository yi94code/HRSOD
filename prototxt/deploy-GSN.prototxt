  bottom: "conv4_score"
  top: "conv4_score"
  relu_param {
    negative_slope: 0.01
  }
}



layer {
  name: "fuse_score1"
  type: "Eltwise"
  bottom: "upscore1"	
  bottom: "conv4_score"
  top: "fuse_score1"
  eltwise_param {
    operation: SUM
  }
}


## upscale 2
layer {
  name: "upscore2"
  type: "Deconvolution"
  bottom: "fuse_score1"
  top: "upscore2"
  param {
    lr_mult: 10
    decay_mult: 1
  }
  convolution_param {
    num_output: 2
    bias_term: false
    kernel_size: 8
    pad: 3
    stride: 2
    weight_filler {
      type: "bilinear"
    }
  }
}

layer {
  name: "bn_upscore2"
  type: "BatchNorm"
  bottom: "upscore2"
  top: "upscore2"
  param {
    lr_mult: 0
  }
  param {
    lr_mult: 0
  }
  param {
    lr_mult: 0
  }
}

layer {
  name: "scale_upscore2"
  type: "Scale"
  bottom: "upscore2"
  top: "upscore2"
