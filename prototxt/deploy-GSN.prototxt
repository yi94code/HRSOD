    lr_mult: 10
    decay_mult: 1
  }
  param {
    lr_mult: 20
    decay_mult: 0
  }
  scale_param {
    bias_term: true
  }
}
layer {
  name: "relu5_3"
  type: "ReLU"
  bottom: "conv5_3"
  top: "conv5_3"
  relu_param {
    negative_slope: 0.01
  }
}
layer {
	bottom: "conv5_3"
	top: "conv5_dcpp_1-sm"
	name: "conv5_dcpp_1-sm"
	param {
		lr_mult: 10
	    decay_mult: 1
	}
	param {
		lr_mult: 20
	    decay_mult: 0
	}
	type: "Convolution"
	convolution_param {
		num_output: 32
		pad: 1
    	dilation: 1
		kernel_size: 3
		weight_filler {
      		#type: "gaussian"
                type: "msra"
	    	}
	    	bias_filler {
			type: "constant"
     			 value: 0
		}
	}
}
layer {
	bottom: "conv5_dcpp_1-sm"
	top: "conv5_dcpp_1-sm"
	name: "relu5_dcpp_1-sm"
	type: "ReLU"
}

layer {
	bottom: "conv5_3"
	top: "conv5_dcpp_3-sm"
	name: "conv5_dcpp_3-sm"
	param {
		lr_mult: 10
	    decay_mult: 1
	}
	param {
		lr_mult: 20
	    decay_mult: 0
	}
	type: "Convolution"
	convolution_param {
		num_output: 32
		pad: 3
    	dilation: 3
		kernel_size: 3
