	param {
		lr_mult: 10
	    decay_mult: 1
	}
	param {
		lr_mult: 20
	    decay_mult: 0
	}
	type: "Convolution"
	convolution_param {
		num_output: 32
		pad: 5
    	dilation: 5
		kernel_size: 3
		weight_filler {
      		#type: "gaussian"
                type: "msra"
	    	}
	    	bias_filler {
			type: "constant"
     			 value: 0
		}
	}
}
layer {
	bottom: "conv4_dcpp_5-sm"
	top: "conv4_dcpp_5-sm"
	name: "relu4_dcpp_5-sm"
	type: "ReLU"
}
layer {
	bottom: "conv4_3"
	top: "conv4_dcpp_7-sm"
	name: "conv4_dcpp_7-sm"
	param {
		lr_mult: 10
	    decay_mult: 1
	}
	param {
		lr_mult: 20
	    decay_mult: 0
	}
	type: "Convolution"
	convolution_param {
		num_output: 32
		pad: 7
    	dilation: 7
		kernel_size: 3
		weight_filler {
      		#type: "gaussian"
                type: "msra"
	    	}
	    	bias_filler {
			type: "constant"
     			 value: 0
		}
	}
}
layer {
	bottom: "conv4_dcpp_7-sm"
	top: "conv4_dcpp_7-sm"
	name: "relu4_dcpp_7-sm"
	type: "ReLU"
}
layer {
	bottom: "conv4_dcpp_1-sm"
	bottom: "conv4_dcpp_3-sm"
	bottom: "conv4_dcpp_5-sm"
	bottom: "conv4_dcpp_7-sm"
	top: "concat_conv4_dcpp"
	name: "concat_conv4_dcpp"
	type: "Concat"
}
layer {
  name: "conv4_score"
  type: "Convolution"
  bottom: "concat_conv4_dcpp"
  top: "conv4_score"
  param {
    lr_mult: 10
    decay_mult: 1
  }
  param {
    lr_mult: 20
    decay_mult: 0
  }
  convolution_param {
    num_output: 2
    pad: 1
    kernel_size: 3
    weight_filler {
      type: "msra"
      #type: "gaussian" #"msra"
      #std: 0.001
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "bn_conv4_score"
  type: "BatchNorm"
  bottom: "conv4_score"
  top: "conv4_score"
  param {
    lr_mult: 0
  }
  param {
    lr_mult: 0
  }
  param {
    lr_mult: 0
