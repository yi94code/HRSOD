	    decay_mult: 1
	}
	param {
		lr_mult: 20
	    decay_mult: 0
	}
	type: "Convolution"
	convolution_param {
		num_output: 32
		pad: 3
    	dilation: 3
		kernel_size: 3
		weight_filler {
      		#type: "gaussian"
                type: "msra"
	    	}
	    	bias_filler {
			type: "constant"
     			 value: 0
		}
	}
}
layer {
	bottom: "conv4_dcpp_3-sm"
	top: "conv4_dcpp_3-sm"
	name: "relu4_dcpp_3-sm"
	type: "ReLU"
}

layer {
	bottom: "conv4_3"
	top: "conv4_dcpp_5-sm"
