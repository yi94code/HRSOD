		}
	}
}
layer {
	bottom: "conv5_dcpp_7-sm"
	top: "conv5_dcpp_7-sm"
	name: "relu5_dcpp_7-sm"
	type: "ReLU"
}
layer {
	bottom: "conv5_dcpp_1-sm"
	bottom: "conv5_dcpp_3-sm"
	bottom: "conv5_dcpp_5-sm"
	bottom: "conv5_dcpp_7-sm"
	top: "concat_conv5_dcpp"
	name: "concat_conv5_dcpp"
	type: "Concat"
}
layer {
  name: "conv5_3_score"
  type: "Convolution"
  bottom: "concat_conv5_dcpp"
  top: "conv5_3_score"
  param {
    lr_mult: 10 #10000
    decay_mult: 1
  }
  convolution_param {
    num_output: 2
    pad: 1
    kernel_size: 3
    weight_filler {
      type: "msra"
    }
    bias_term: false
    #bias_filler {
    #  type: "constant"
    #  value: 0
    #}
  }
}
#
layer {
  name: "bn_conv5_3_score"
  type: "BatchNorm"
  bottom: "conv5_3_score"
  top: "conv5_3_score"
  param {
    lr_mult: 0
  }
  param {
    lr_mult: 0
  }
