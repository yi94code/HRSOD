	type: "ReLU"
}
layer {
	bottom: "conv5_3"
	top: "conv5_dcpp_7-sm"
	name: "conv5_dcpp_7-sm"
	param {
		lr_mult: 10
	    decay_mult: 1
	}
	param {
		lr_mult: 20
	    decay_mult: 0
	}
	type: "Convolution"
	convolution_param {
		num_output: 32
		pad: 7
    	dilation: 7
		kernel_size: 3
